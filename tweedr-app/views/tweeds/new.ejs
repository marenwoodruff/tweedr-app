<h1>add a new tweed</h1>

<form action="/tweeds" method="POST">
  <div>
    <label for="name">name</label>
    <input type="text" id="name" name="name">
  </div>

  <div>
    <label for="upVote">up vote</label>
    <input type="number" id="upVote" name="upVote" value="0">
  </div>

  <div>
    <label for="downVote">down vote</label>
    <input type="number" id="downVote" name="downVote" value="0">
  </div>

  <input type="submit" value="submit">
</form>
